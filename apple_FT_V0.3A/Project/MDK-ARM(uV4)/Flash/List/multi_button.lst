N            handle->state = 2;
N
N        }
N        else if(handle->ticks > LONG_TICKS)
X        else if(handle->ticks > (2000 / 10))
N        {
N            handle->event = (uint8_t)LONG_PRESS_START;
N            EVENT_CB(LONG_PRESS_START);
X            if(handle ->cb[LONG_PRESS_START]) handle ->cb[LONG_PRESS_START]((button*)handle);
N            handle->state = 5;
N        }
N        break;
N
N    case 2:
N        if(handle->button_level == handle->active_level)
N        {
N            handle->event = (uint8_t)PRESS_DOWN;
