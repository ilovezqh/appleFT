N        break;
N
N    case 3:
N        if(handle->button_level != handle->active_level)
N        {
N            handle->event = (uint8_t)PRESS_UP;
N            EVENT_CB(PRESS_UP);
X            if(handle ->cb[PRESS_UP]) handle ->cb[PRESS_UP]((button*)handle);
N
N            if(handle->ticks < SHORT_TICKS)
X            if(handle->ticks < (300 / 10))
