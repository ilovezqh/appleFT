N/**
N  * @brief  Start the button work, add the handle into work list.
N  * @param  handle: target handle strcut.
N  * @retval 0: succeed. -1: already exist.
N  */
Nint button_start(struct button* handle)
N{
N    struct button* target = head_handle;
N
N    while(target)
N    {
N        if(target == handle)
N        {
N            return -1;  //already exist.
N        }
N
N        target = target->next;
N    }
N
N    handle->next = head_handle;
N    head_handle = handle;
N
N    return 0;
N}
N
N/**
N  * @brief  Stop the button work, remove the handle off work list.
N  * @param  handle: target handle strcut.
N  * @retval None
N  */
Nvoid button_stop(struct button* handle)
N{
N    struct button** curr;
N
N    for(curr = &head_handle; *curr;)
N    {
N        struct button* entry = *curr;
N
N        if (entry == handle)
N        {
N            *curr = entry->next;
N        }
N        else
N        {
N            curr = &entry->next;
N        }
N    }
N}
N
N/**
N  * @brief  background ticks, timer repeat invoking interval 5ms.
N  * @param  None.
N  * @retval None
N  */
Nvoid button_ticks(void)
N{
N    struct button* target;
N
N    for(target = head_handle; target != NULL; target = target->next)
X    for(target = head_handle; target != 0; target = target->next)
N    {
N        button_handler(target);
N    }
N}
