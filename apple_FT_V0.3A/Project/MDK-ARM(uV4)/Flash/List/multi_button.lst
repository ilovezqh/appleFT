X            if(handle ->cb[PRESS_DOWN]) handle ->cb[PRESS_DOWN]((button*)handle);
N            handle->repeat++;
N            EVENT_CB(PRESS_REPEAT);
X            if(handle ->cb[PRESS_REPEAT]) handle ->cb[PRESS_REPEAT]((button*)handle);
N            handle->ticks = 0;
N            handle->state = 3;
N        }
N        else if(handle->ticks > SHORT_TICKS)
X        else if(handle->ticks > (300 / 10))
N        {
N            if(handle->repeat == 1)
